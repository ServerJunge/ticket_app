<div class="col-md-12">
  <% @tickets.each do |ticket| %>
    <h3>
      <%= ticket.name %>
      <small><%= ticket.client %></small>
      <div class="pull-right">
        <%= link_to edit_ticket_path(ticket), class: "icon-link" do %>
          <small><i class="glyphicon glyphicon-cog"></i></small>
        <% end %>
        <small><i class="glyphicon glyphicon-plus-sign"></i></small>
      </div>
    </h3>

    <ul class="list-group">
      <li class="list-group-item">No entries yet!</li>
    </ul>
  <% end %>
</div>
